{
  "manifest_version": 3,
  "name": "Get tiktok cookies extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "tiktok-icon.png"
  },
  "permissions": [
    "cookies"
  ],
  "host_permissions": ["*://*.tiktok.com/"],
  "incognito": "split"
}